{
  "name": "tree-house-authentication",
  "version": "2.2.0",
  "description": "Tree House Authentication module",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./build/*",
    "build": "npm run clean && tsc",
    "lint": "tslint 'src/**/*.ts'",
    "test": "jest --forceExit",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/**/*.ts --forceExit",
    "test:watch": "jest --watchAll",
    "pretest": "npm run build",
    "prepare": "npm run build",
    "release": "node_modules/np/cli.js --no-cleanup"
  },
  "precommit": [
    "lint"
  ],
  "dependencies": {
    "express-session": "~1.15.6",
    "jsonwebtoken": "~8.2.2",
    "qrcode": "~1.2.0",
    "speakeasy": "~2.0.0"
  },
  "devDependencies": {
    "@types/express-session": "~1.15.9",
    "@types/jest": "~22.2.0",
    "@types/jsonwebtoken": "~7.2.5",
    "@types/qrcode": "~0.8.1",
    "@types/speakeasy": "~2.0.2",
    "coveralls": "^3.0.0",
    "jest": "^23.1.0",
    "np": "^3.0.1",
    "pre-commit": "^1.2.2",
    "supertest": "^3.1.0",
    "ts-jest": "~22.4.6",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^2.9.1"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "tree House",
    "treehouse",
    "authentication",
    "passport",
    "passport-local",
    "passport-jwt"
  ],
  "license": "ISC",
  "repository": "git@github.com:icapps/tree-house-authentication.git",
  "bugs": {
    "url": "https://github.com/icapps/tree-house-authentication/issues"
  },
  "homepage": "https://github.com/icapps/tree-house-authentication#readme"
}
